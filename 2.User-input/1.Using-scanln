//User input Golang
package main

import "fmt"

func main() {
	var num int
	fmt.Println("Enter the number:")
	fmt.Scanln(&num)
	fmt.Printf("The entered number is :%v\n", num)

	var name string
	fmt.Println("Enter the name:")
	fmt.Scanln(&name)
	fmt.Printf("The entered name is :%v\n", name)

}


